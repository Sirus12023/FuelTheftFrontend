{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api(?:/(.*))$",
      "dest": "http://3.81.250.43:3000/$1",
      "check": true
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/",
      "check": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "framework": {
    "version": "7.0.4"
  },
  "crons": []
}
